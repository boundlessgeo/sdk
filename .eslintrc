{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
        "no-console": ["error", {
            "allow" : ["warn", "error"]
        }],
        "no-plusplus" : ["error", {
            "allowForLoopAfterthoughts": true,
        }],
        "camelcase": 0
    }
}
